include ~/.cache/wal/colors-kitty.conf
# font_family         JetBrainsMono NF Medium
# bold_font           JetBrainsMono NF Bold
# italic_font         JetBrainsMono NF Italic
# bold_italic_font    JetBrainsMono NF Bold Italic

font_size 10 

open_url_with       librewolf

copy_on_select      yes

background_opacity  0.85


# BEGIN_KITTY_FONTS
font_family      family='JetBrainsMono Nerd Font' style=Medium
bold_font        family='JetBrainsMono Nerd Font' postscript_name=JetBrainsMonoNF-Bold
italic_font      auto
bold_italic_font auto
# END_KITTY_FONTS